CREATE TABLE namibia_prod.dhis2_disaggregations
(
    id serial not null,
    uid character varying(15),
    name character varying(50),
    reveal_uuid character varying(40),
    active boolean NOT NULL DEFAULT true,
    CONSTRAINT dhis2_disaggregations_pkey PRIMARY KEY (id)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE namibia_prod.dhis2_disaggregations
    OWNER to postgres;

CREATE UNIQUE INDEX idx_dis_uid
    ON namibia_prod.dhis2_disaggregations USING btree
    (uid ASC NULLS LAST)
    TABLESPACE pg_default;
